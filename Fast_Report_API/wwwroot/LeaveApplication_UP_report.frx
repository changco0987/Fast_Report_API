    private void _StartReport(object sender, EventArgs e)
    {
         getEducationDetails();
      Pay();
      leaveDetails();
    }
    
    //To get all data from education datasource
    private void getEducationDetails()
    { 
      // get a reference to the data source
      DataSourceBase ds = Report.GetDataSource(&quot;leave_names&quot;);
                                                                        
      int index = 1;
 

      // initialize it
      ds.Init();    

      // enum all rows
      while (ds.HasMoreRows)
      {

        // get the data column value from the current row                     
        string leaveName = ((String)Report.GetColumnValue(&quot;leave_names.value&quot;));   
        string selectedLeaveName = ((String)Report.GetColumnValue(&quot;LeaveAppPrint.leave_type_name&quot;));
        string leave_remarks = ((String)Report.GetColumnValue(&quot;LeaveAppPrint.other_remarks&quot;));                                                                                       
        if(index == 1)
        {                                 
          Text9.Text = leaveName;   
          if(leaveName == selectedLeaveName)
          {         
            CheckBox1.Checked = true;      
          }
        }
        else if(index == 2)
        {                                     
          Text10.Text = leaveName;    
          if(leaveName == selectedLeaveName)
          {         
            CheckBox2.Checked = true;      
          }
        } 
        else if(index == 3)
        {                                     
          Text11.Text = leaveName;    
          if(leaveName == selectedLeaveName)
          {         
            CheckBox3.Checked = true;      
          }
        } 
        else if(index == 4)
        {                                     
          Text12.Text = leaveName; 
          if(leaveName == selectedLeaveName)
          {         
            CheckBox4.Checked = true;      
          }
        } 
        else if(index == 5)
        {                                     
          Text13.Text = leaveName;   
          if(leaveName == selectedLeaveName)
          {         
            CheckBox5.Checked = true;
            Text50.Text =leave_remarks;
          }
        } 
     
       
       
        
        
        
        
        
        

        index++;
 

        // go next data row
        ds.Next();

      }     
      
     
     
    }
    
    private void Pay (){
      string status = ((String)Report.GetColumnValue(&quot;LeaveAppPrint.status&quot;));   
      string remarks = ((String)Report.GetColumnValue(&quot;LeaveAppPrint.remarks&quot;));
      
      if( status.ToUpper().StartsWith(&quot;APPROVED&quot;)){
        CheckBox8.Checked = true;
        CheckBox6.Checked = true;
      }else{
        CheckBox9.Checked = true;
        CheckBox6.Checked = true;
      }
      if(status.ToUpper().StartsWith(&quot;DECLINED (DEPARTMENT)&quot;)){
        Text48.Text = remarks;
      }else{
        Text49.Text = remarks;
      }
      
    }
    
    private void leaveDetails (){
      string leave_details = ((String)Report.GetColumnValue(&quot;LeaveAppPrint.leaveDetails&quot;)); 
      int within_phil = leave_details.ToUpper().IndexOf(&quot;WITHIN THE PHILIPPINES:&quot;);
      int abroad = leave_details.ToUpper().IndexOf(&quot;ABROAD&quot;);
      int in_hosp = leave_details.ToUpper().IndexOf(&quot;IN HOSPITAL&quot;);
      int out_patient = leave_details.ToUpper().IndexOf(&quot;OUT PATIENT&quot;);  
      
      if (within_phil != -1)
      {
        CheckBox10.Checked = true;
        string result = leave_details.ToUpper().Replace(&quot;WITHIN THE PHILIPPINES: &quot;, &quot;&quot;);
        Text32.Text = result;
      }
      if(abroad != -1){
        CheckBox11.Checked = true; 
        string result = leave_details.ToUpper().Replace(&quot;ABROAD: &quot;, &quot;&quot;);
        Text33.Text = result;
      }
      if(in_hosp != -1){
        CheckBox12.Checked = true;
        string result = leave_details.ToUpper().Replace(&quot;IN HOSPITAL: &quot;, &quot;&quot;);
        Text34.Text = result;
      }
      if(out_patient != -1){
        CheckBox13.Checked = true;
        string result = leave_details.ToUpper().Replace(&quot;OUT PATIENT: &quot;, &quot;&quot;);
        Text35.Text = result;
      }
      
     
      
      
    }
    

   
  }
  
  
  
  
}

