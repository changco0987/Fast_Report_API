    private void Pay (){
        string status = ((String)Report.GetColumnValue(&quot;LeaveAppPrint.status&quot;));   
        string remarks = ((String)Report.GetColumnValue(&quot;LeaveAppPrint.remarks&quot;));
      
      if( status.ToUpper().StartsWith(&quot;APPROVED&quot;)){
        CheckBox17.Checked = true;
        CheckBox15.Checked = true;
      }else{
        CheckBox18.Checked = true;
        CheckBox15.Checked = true;
      }
      
      if(status.ToUpper().StartsWith(&quot;DECLINED (DEPARTMENT)&quot;)){
         Text83.Text = remarks;
      
      }else{
            Text84.Text = remarks;
      }
      
    }
    
    private void leaveDetails (){
      string leave_details = ((String)Report.GetColumnValue(&quot;LeaveAppPrint.leaveDetails&quot;));
      int within_phil = leave_details.ToUpper().IndexOf(&quot;WITHIN THE PHILIPPINES:&quot;);
      int abroad = leave_details.ToUpper().IndexOf(&quot;ABROAD&quot;);
      int in_hosp = leave_details.ToUpper().IndexOf(&quot;IN HOSPITAL&quot;);
      int out_patient = leave_details.ToUpper().IndexOf(&quot;OUT PATIENT&quot;);  
      int master_deg = leave_details.ToUpper().IndexOf(&quot;COMPLETION OF MASTER'S DEGREE:&quot;); 
      int board_exam = leave_details.ToUpper().IndexOf(&quot;BAR/BOARD EXAMINAION:&quot;); 
      int monetize = leave_details.ToUpper().IndexOf(&quot;MONETIZATION OF LEAVE CREDITS:&quot;); 
      int terminal = leave_details.ToUpper().IndexOf(&quot;TERMINAL LEAVE:&quot;);
      if (within_phil != -1)
      {
        CheckBox22.Checked = true;
        string result = leave_details.ToUpper().Replace(&quot;WITHIN THE PHILIPPINES: &quot;, &quot;&quot;);
        Text74.Text = result;
      }
      if(abroad != -1){
        CheckBox23.Checked = true; 
        string result = leave_details.ToUpper().Replace(&quot;ABROAD: &quot;, &quot;&quot;);
        Text75.Text = result;
      }
      if(in_hosp != -1){
        CheckBox24.Checked = true; 
        string result = leave_details.ToUpper().Replace(&quot;IN HOSPITAL: &quot;, &quot;&quot;);
        Text76.Text = result;
      }
      if(out_patient != -1){
        CheckBox25.Checked = true;
        string result = leave_details.ToUpper().Replace(&quot;OUT PATIENT &quot;, &quot;&quot;);
        Text77.Text = result;
      }
      if(master_deg != -1){
        CheckBox26.Checked = true; 
        string result = leave_details.ToUpper().Replace(&quot;COMPLETION OF MASTER'S DEGREE &quot;, &quot;&quot;);
        Text78.Text = result;
      }
      if(board_exam != -1){
        CheckBox27.Checked = true; 
        string result = leave_details.ToUpper().Replace(&quot;BAR/BOARD EXAMINAION &quot;, &quot;&quot;);
        Text79.Text = result;
      }
      if(monetize != -1){
        CheckBox28.Checked = true; 
        string result = leave_details.ToUpper().Replace(&quot;MONETIZATION OF LEAVE CREDITS &quot;, &quot;&quot;);
        Text80.Text = result;
      }
      if(terminal != -1){
        CheckBox29.Checked = true; 
        string result = leave_details.ToUpper().Replace(&quot;TERMINAL LEAVE &quot;, &quot;&quot;);
        Text81.Text = result;
      }
     
      
      
    }
    
